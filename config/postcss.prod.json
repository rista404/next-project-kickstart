{
    "use": [
        "postcss-import",
        "postcss-nesting",
        "cssnano",
        "postcss-cssnext"
    ],
    "input": "src/css/main.css",
    "output": "dist/bundle.min.css",
    "local-plugins": true,
    "cssnext": {
        "browsers": "> 5%"
    },
    "cssnano": {
        "discardComments": {
          "removeAll": true
        }
    }
}